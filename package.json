{
  "name": "gitnab",
  "version": "1.1.0",
  "description": "Nab just what you need from any GitHub repo - extract subfolders with ease",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gitnab": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "node --import tsx --test src/*.test.ts",
    "test:coverage": "c8 --reporter=lcov --reporter=text node --import tsx --test src/*.test.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "github",
    "extract",
    "subfolder",
    "download",
    "cli",
    "tarball"
  ],
  "author": "Alan Tondelier <alan@221b.fr>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alan-221b/gitnab.git"
  },
  "homepage": "https://github.com/Alan-221b/gitnab#readme",
  "bugs": {
    "url": "https://github.com/Alan-221b/gitnab/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "tsx": "^4.19.0",
    "@types/node": "^22.13.4",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3"
  }
}
